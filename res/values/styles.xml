<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="PopupwindowAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/popupwindow_enter</item>
        <item name="android:windowExitAnimation">@anim/popupwindow_exit</item>
    </style>

    <style name="FoundMydtPopupwindowAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/foundmydtpopupwindow_enter</item>
        <item name="android:windowExitAnimation">@anim/foundmydtpopupwindow_exit</item>
    </style>

    <style name="app_bg">
        <item name="android:background">#f4f4f4</item>
    </style>

    <!-- list 样式 -->
    <style name="list_Style">
        <item name="android:divider">@null</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:scrollbars">@null</item>
    </style>

    <style name="anim_popup_dir">
        <item name="android:windowEnterAnimation">@anim/picselect_slide_in</item>
        <item name="android:windowExitAnimation">@anim/picselect_slide_out</item>
    </style>

